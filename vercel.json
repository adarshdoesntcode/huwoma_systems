{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://huwoma-backend.vercel.app/api/:path*"
    },

    {
      "source": "/(.*)",
      "destination": "/index.html",
      "has": [
        {
          "type": "header",
          "key": "Accept",
          "value": "(.*)text/html(.*)"
        }
      ]
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
