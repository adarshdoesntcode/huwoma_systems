{
  "redirects": [
    {
      "source": "/api/oauth/google/:path*",
      "destination": "https://huwoma-backend.vercel.app/api/oauth/google/:path*",
      "permanent": false
    }
  ],
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://huwoma-backend.vercel.app/api/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
